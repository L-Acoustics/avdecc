{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AEM Schema",
  "type": "object",
  "description": "JSON validation schema for ATDECC Entity Model files",
  "schema_version": "2024.10.16",
  "properties": {
    "_dump_source (informative)": {
      "type": "string",
      "description": "Name and Version of the tool used to generate the dump."
    },
    "dump_version": {
      "type": "integer",
      "minimum": 1,
      "maximum": 1
    },
    "entity_model": {"$ref": "#/$aemFormat/entityModel"},
    "entity_model_id": {"$ref": "#/$commonDefinitions/uniqueIdentifier"},
    "entity_capabilities": {
      "type": "array",
      "items": {
        "$ref": "#/$commonDefinitions/entityCapability"
      }
    },
    "talker_stream_sources": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "talker_capabilities": {
      "type": "array",
      "items": {
        "$ref": "#/$commonDefinitions/talkerCapability"
      }
    },
    "listener_stream_sinks": {
      "type": "array",
      "items": {
          "type": "string"
      }
    },
    "listener_capabilities": {
      "type": "array",
      "items": {
        "$ref": "#/$commonDefinitions/listenerCapability"
      }
    },
    "controller_capabilities": {
      "type": "array",
      "items": {
        "$ref": "#/$commonDefinitions/controllerCapability"
      }
    },
    "identify_control_index": {
      "$ref": "#/$commonDefinitions/descriptorIndex"
    },
    "association_id": {
      "$ref": "#/$commonDefinitions/descriptorIndex"
    }
  },
  "$commonDefinitions": {
    "uniqueIdentifier": {
      "type": "string",
      "pattern": "^0x[0-9a-fA-F]{16}$",
      "description": "Defined as a string representing an hexadecimal 64 bits value."
    },
    "avdeccString": {
      "type": "string",
      "maxLength": 64,
      "description": "Defined as a string with a maximum length of 64 bytes.(not characters, if possible in JSON Schema)"
    },
    "descriptorIndex": {
      "type": "integer",
      "minimum": 0,
      "maximum": 65535,
      "description": "Defined as a number ranging from 0 to 65535."
    },
    "uint8": {
      "type": "integer",
      "minimum": 0,
      "maximum": 255,
      "description": "Defined as an unsigned one byte number (ranging from 0 to 255)."
    },
    "uint16": {
      "type": "integer",
      "minimum": 0,
      "maximum": 65535,
      "description": "Defined as an unsigned two bytes number (ranging from 0 to 65535)."
    },
    "uint32": {
      "type": "integer",
      "minimum": 0,
      "maximum": 4294967295,
      "description": "Defined as an unsigned four bytes number (ranging from 0 to 4294967295)."
    },
    "uint64": {
      "type": "integer",
      "minimum": 0,
      "maximum": 18446744073709551615,
      "description": "Defined as an unsigned 8 bytes number."
    },
    "macAddress": {
      "type": "string",
      "pattern": "^([A-F0-9]{2}:){5}[A-F0-9]{2}$",
      "description": "Defined as a string representing 6 hexadecimal bytes separated by a colon."
    },
    "streamFormat": {
      "type": "string",
      "pattern": "^0x[0-9a-fA-F]{16}$",
      "description": "Defined as a string representing an hexadecimal 64 bits value."
    },
    "samplingRate": {
      "type": "string",
      "pattern": "^0x[0-9a-fA-F]{8}$",
      "description": "Defined as a string representing an hexadecimal 32 bits value."
    },
    "localizedDescription": {
      "type": "object",
      "properties": {
        "index": {
          "type": "integer",
          "minimum": 0,
          "maximum": 7
        },
        "offset": {
          "type": "integer",
          "minimum": 0,
          "maximum": 8191
        }
      },
      "required": ["index", "offset"],
      "description": "An object with the following properties of index (0-7) and offset (0-8191) all required"
    },
    "audioMapping": {
      "type": "object",
      "properties": {
        "stream_index": {
          "$ref": "#/$commonDefinitions/descriptorIndex"
        },
        "stream_channel": {
          "$ref": "#/$commonDefinitions/uint16"
        },
        "cluster_offset": {
          "$ref": "#/$commonDefinitions/descriptorIndex"
        },
        "cluster_channel": {
          "$ref": "#/$commonDefinitions/uint16"
        }
      },
      "required": ["stream_index", "stream_channel", "cluster_offset", "cluster_channel"],
      "description": "As an object with the following properties of StreamIndex (descriptor Index), stream Channel (uint16), cluster offset (descriptorIndex), and cluster channel (uint16) all required"
    },
    "entityCapability":{
      "type": "string",
      "enum": [
        "UNKNOWN",
        "EFU_MODE",
        "ADDRESS_ACCESS_SUPPORTED",
        "GATEWAY_ENTITY",
        "AEM_SUPPORTED",
        "LEGACY_AVC",
        "ASSOCIATION_ID_SUPPORTED",
