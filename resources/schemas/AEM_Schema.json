  "$aemFormat": {
    "entityModel": {
      "type": "object",
      "properties": {
        "not_compliant": {"type": "boolean"},
        "entity_descriptor": {"$ref": "#/$aemFormat/entityDescriptor"}
      },
      "required": ["entity_descriptor"]
    },
    "entityDescriptor": {
      "type": "object",
      "properties": {
        "configuration_descriptors": {"oneOf": [
          {
          "type": "array",
          "items": {"$ref": "#/$aemFormat/configurationDescriptor"}
          },
          {
          "type": "null"
          }
        ]},
        "static": {"$ref": "#/$aemFormat/entityDescriptorStaticModel"}
      },
      "required": ["configuration_descriptors","static"]
    },
    "entityDescriptorStaticModel": {
      "type": "object",
      "properties": {
        "model_name_string": {"$ref": "#/$commonDefinitions/localizedDescription"},
        "vendor_name_string": {"$ref": "#/$commonDefinitions/localizedDescription"}
      },
      "required": ["model_name_string", "vendor_name_string"]
    },
    "configurationDescriptor": {
      "type": "object",
      "properties": {
        "_index (informative)": {"$ref": "#/$commonDefinitions/descriptorIndex"},
        "static": {"$ref": "#/$aemFormat/configurationDescriptorStaticModel"},
        "audio_unit_descriptors": {"oneOf": [
          {
          "type": "array",
          "items": {"$ref": "#/$aemFormat/audioUnitDescriptor"}
          },
          {
          "type": "null"
          }
        ]},
        "stream_input_descriptors": {"oneOf": [
          {
          "type": "array",
          "items": {"$ref": "#/$aemFormat/streamDescriptor"}
          },
          {
          "type": "null"
          }
        ]},
        "stream_output_descriptors": {"oneOf": [
          {
          "type": "array",
          "items": {"$ref": "#/$aemFormat/streamDescriptor"}
          },
          {
          "type": "null"
          }
        ]},
        "avb_interface_descriptors": {"oneOf": [
          {
          "type": "array",
          "items": {"$ref": "#/$aemFormat/avbInterfaceDescriptor"}
          },
          {
          "type": "null"
          }
        ]},
        "clock_source_descriptors": {"oneOf": [
          {
          "type": "array",
          "items": {"$ref": "#/$aemFormat/clockSourceDescriptor"}
          },
          {
          "type": "null"
          }
